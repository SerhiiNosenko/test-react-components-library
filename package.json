  "types": "dist/index.d.ts",
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest",
